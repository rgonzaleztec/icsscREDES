
<resources>
    <!-- Customizations -->

    <color name="textColor">#f6f0f0</color>
    <color name="textColorSecondary">#99f6f0f0</color>
    <color name="textColorMuted">#4df6f0f0</color>
    <color name="textBarColor">#ffffff</color>
    <color name="hintSearchBarColor">#99ffffff</color>
    <color name="window_background">#1c1b1b</color>
   
    <!-- background drawable: be sure it has a not too large size/resolution -->
    <drawable name="activity_background_image">@drawable/jpg_fondo615</drawable>
    
    <!-- toolbar background -->
    <color name="actionbar_background">#bf6767</color>

    <!-- status bar background (android 5.x) -->
    <color name="primaryDark">#ff9b4141</color>
    
    <!-- list selections -->
    <color name="selectionColor">#44f6f0f0</color>

    <!-- card style -->
    <drawable name="background_card">@drawable/bg_dark_card</drawable>

    <!-- card view -->
    <color name="cardBackgroundColor">#991c1b1b</color>

    <!-- icons -->
    <drawable name="ic_device_access_call">@drawable/ic_device_access_call_light</drawable>
    <drawable name="ic_location_web_site">@drawable/ic_location_web_site_light</drawable>
    <drawable name="ic_location_place">@drawable/ic_location_place_light</drawable>
    <drawable name="ic_content_email">@drawable/ic_content_email_light</drawable>
	<drawable name="ic_keyboard_arrow_right">@drawable/ic_keyboard_arrow_right_light</drawable>

</resources>

